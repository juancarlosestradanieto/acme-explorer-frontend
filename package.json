{
  "name": "acme-explorer-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config-env && ng serve",
    "build": "npm run config-env && ng build",
    "watch": "npm run config-env && ng build --watch --configuration development",
    "test": "npm run config-env && ng test",
    "test-headless": "npm run config-env && ng test --browsers ChromeHeadlessNoSandbox",
    "fake-backend": "json-server --watch src/assets/json-server/acme-explorer.json",
    "fake-backend-routes": "json-server --watch src/assets/json-server/acme-explorer.json routes src/assets/json-server/routes.json",
    "config-env": "ts-node src/environments/set-env.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^13.0.0",
    "@angular/common": "^14.2.0",
